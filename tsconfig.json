{
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "baseUrl": "src",
    "rootDir": "src",
    "target": "ES2022",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.ts",
  ]
}